let g:zenburn_high_contrast=1
colors zenburn
set textwidth=90
set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set cindent
set nu
set noexpandtab
set hlsearch
syntax on
filetype plugin indent on

set laststatus=2
set statusline=%F%m%r%h%w\ [%{&ff}]\ [%Y]\ [%l,%v][%p%%]\ [%L]
let mapleader="`"
